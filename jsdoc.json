{
  "source": {
      "include": ["./"],
      "exclude": ["./node_modules", "docs"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "node_modules/better-docs/typescript"
  ],
  "opts": {
      "destination": "../doc-simulador/doc-fonte/public/",
      "template": "node_modules/better-docs"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
       "better-docs": {
        "name": "Painel Documentation",
        "logo": "images/painel-fiscal-horizontal-dark.png",
        "title": "Painel Fiscal", 
        "css": "styles/mystyle.css",
        "trackingCode": "",
        "hideGenerator": false,
        "navLinks": [
            {
                "label": "Github",
                "href": "https://github.com/matheusmello18"
            },
            {
                "label": "Simulador da Cat42",
                "href": "http://wwww.simuladorcat42.com.br/"
            }
        ]
    }
  }
}